{
  "name": "backend",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.13.1"
  },
  "main": "index.js",
  "bin": "src/index.js",
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "assets": [
      "../frontend/build/**/*",
      "uploads/**/*",
      "node_modules/pino/**/*",
      "node_modules/thread-stream/**/*",
      "node_modules/pino-pretty/**/*"
    ],
    "output": "dist"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg .",
    "build:linux": "pkg . --targets node18-linux-x64"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}